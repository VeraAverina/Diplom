<h3>Изменить оценки</h3>
<% if !@resultfile.errors.empty? %>
   <div class = "alert alert-error">
      
      <ul>
         <% @resultfile.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
         <% end %>
      </ul>
      
   </div>
<% end %>
<%= form_for( :resultfile, url: resultfile_path(@resultfile), method: :patch) do |f| %>
  
  <p>
  <% if @resultfile.errors.any? %>
  <% @resultfile.errors.full_messages.each do |msg| %>
    <div class="alert alert-danger"><%= msg %></div>
  <% end %>
<% end %>

<p>Оценка<br>
  <%= f.text_field(:mark, {:class => 'form-control'} )%>
</p>
<p>
  Комментарий<br>
  <%= f.text_area(:comment, {:class => 'form-control'} ) %>
</p>
    <%= f.submit('Запомнить', {:class => 'bot1'}) %>
    
  </p>
<% end %>
